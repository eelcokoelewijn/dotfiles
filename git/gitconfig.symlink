# Local/private config goes in the include
[include]
        path = ~/.gitconfig.local
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
        editor = vim
[diff]
        tool = opendiff
[merge]
        tool = opendiff
[difftool]
        prompt = false
[help]
        autocorrect = 1
[pull]
        rebase = false
[init]
        defaultBranch = main
